{
    "name": "tobinfree/neo4j-bundle",
    "description": "Symfony integration for Neo4j",
    "type": "symfony-bundle",
    "keywords": ["neo4j"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Nyholm",
            "email": "tobias.nyholm@gmail.com"
        },
        {
            "name": "Xavier Coureau",
            "email": "xavier@pandawan-technology.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/2binfree/neo4j-php-client"
        },
        {
            "type": "vcs",
            "url": "https://github.com/2binfree/neo4j-bolt-php"
        }
    ],
    "require": {
        "php": "^8.2",
        "tobinfree/neo4j-bolt": "^1.11.2",
        "tobinfree/neo4j-php-client": "^4.8.8",
        "symfony/dependency-injection": " ^2.8.3 || ^3.0.3 || ^4.0",
        "symfony/framework-bundle": "^2.8.32 || ^3.0 || ^4.0",
        "symfony/http-kernel": "^2.8 || ^3.0 || ^4.0",
        "twig/twig": "^1.18 || ^2.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.3 || ^4.0",
        "matthiasnoback/symfony-dependency-injection-test": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Neo4j\\Neo4jBundle\\": ""
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "scripts": {
        "test": "vendor/bin/simple-phpunit",
        "test-ci": "vendor/bin/simple-phpunit --coverage-text --coverage-clover=build/coverage.xml"
    }
}
